	{
	"name": "calva-extras",
	"displayName": "calva-extras",
	"description": "https://calva.io/extras",
	"version": "0.0.1",
	"publisher": "clyfe",
	"repository": {
		"type": "git",
		"url": "https://github.com/clyfe/calva-extras.git"
  },
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:clojure"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "calva-extras.killToClipboard",
				"title": "Calva Extras: Kill To Clipboard"
			},
			{
				"command": "calva-extras.killSpaceBackward",
				"title": "Calva Extras: Kill Space Backward"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.52.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.15.0",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1"
	}
}
